<table>
  <tr>
    <th>Forum name</th>
    <th>Forum url</th>
    <th>Forum permissions</th>
  </tr>

<% @forums.each do |forum| %>
  <tr>
    <td><%=h forum.forum_name %></td>
    <td><%=h forum.forum_url %></td>
    <td><%=h forum.forum_permissions %></td>
    <td><%= link_to 'Show', forum %></td>
    <td><%= link_to 'Edit', edit_forum_path(forum) %></td>
    <td><%= link_to 'Destroy', forum, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

