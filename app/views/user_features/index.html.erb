<p id="notice"><%= notice %></p>

<h1>Listing User Features</h1>

<table>
  <thead>
    <tr>
      <th>Map layer</th>
      <th>User</th>
      <th>Name</th>
      <th>Text</th>
      <th>Number</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_features.each do |user_feature| %>
      <tr>
        <td><%= user_feature.map_layer_id %></td>
        <td><%= user_feature.user_id %></td>
        <td><%= user_feature.name %></td>
        <td><%= user_feature.text %></td>
        <td><%= user_feature.number %></td>
        <td><%= user_feature.amount %></td>
        <td><%= link_to 'Show', user_feature %></td>
        <td><%= link_to 'Edit', edit_user_feature_path(user_feature) %></td>
        <td><%= link_to 'Destroy', user_feature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User feature', new_user_feature_path %>
