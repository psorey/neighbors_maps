
 <div  class="grid" height="100%"> 
  <% maps.each do |theme_map| %>
      <div class="row">
        <div class="col-md-3"><%=h link_to theme_map.name, "theme_maps/#{theme_map.slug}" %></div>
        <div class="col-md-2">
          <%= link_to "theme_maps/#{theme_map.slug}", title:"#{theme_map.slug}"  do %>
            <%=image_tag theme_map.thumbnail_url, class: "img-responsive imageClip" %>
          <% end %>
        </div>
        <div class="col-md-5"><%=h theme_map.get_summary_description %></div>
        <div class="col-md-1"><%= link_to 'Edit', edit_theme_map_path(theme_map) %></div>       
        <div class="col-md-1"><%= link_to 'Destroy', theme_map, data: { confirm: 'Really?', method: :delete } %></div>
      </div>
  <% end %> 

 </div>


 
