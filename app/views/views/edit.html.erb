<h1>Editing view</h1>

<% form_for(@view) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :owner_id %><br />
    <%= f.text_field :owner_id %>
  </p>
  <p>
    <%= f.label :published %><br />
    <%= f.text_field :published %>
  </p>
  <p>
    <%= f.label :map_layer_list %><br />
    <%= f.text_area :map_layer_list %>
  </p>
  <p>
    <%= f.label :scale %><br />
    <%= f.text_field :scale %>
  </p>
  <p>
    <%= f.label :lower_left %><br />
    <%= f.text_field :lower_left %>
  </p>
  <p>
    <%= f.label :upper_right %><br />
    <%= f.text_field :upper_right %>
  </p>
  <p>
    <%= f.label :mapfile_name %><br />
    <%= f.text_field :mapfile_name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @view %> |
<%= link_to 'Back', views_path %>